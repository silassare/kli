{
	"name": "silassare/kli",
	"description": "Kli helps you add an interactive command-line interface to your php project.",
	"version": "2.0.0",
	"keywords": [
		"kli",
		"cli",
		"command",
		"command-line",
		"command-line-tool",
		"php-cli"
	],
	"require": {
		"php": ">=7.4",
		"ext-readline": "*",
		"ext-posix": "*",
		"ext-mbstring": "*"
	},
	"require-dev": {
	  "phpunit/phpunit": "^9.5.9",
	  "oliup/oliup-cs-php": "dev-main",
	  "roave/security-advisories": "dev-latest",
	  "vimeo/psalm": "^4.21"
	},
	"homepage": "https://github.com/silassare/kli#readme",
	"autoload": {
		"psr-4": {
			"Kli\\": "src"
		}
	},
	"authors": [
		{
			"name": "Emile Silas Sare",
			"email": "emile.silas@gmail.com",
			"homepage": "https://github.com/silassare"
		}
	],
	"license": "MIT",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"branch-alias": {
			"dev-dev": "2.0.x-dev"
		}
	}
}
